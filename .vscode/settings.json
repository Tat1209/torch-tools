        "markdown-pdf.footerTemplate": "<div style=\\"position: relative; border-top: 1px solid black; margin: 0.5cm; font-size: 9px; width: 100%;\\"><div style=\\"position: absolute; width: 100%; top: 0.2cm; text-align: center;\\"><span class='pageNumber'></span> / <span class='totalPages'></span></div><div style=\\"position: absolute; right: 0; top: 0.2cm;\\">Footer test.</div></div>",
